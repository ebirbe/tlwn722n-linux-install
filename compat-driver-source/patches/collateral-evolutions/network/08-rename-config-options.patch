 obj-$(CONFIG_ATH_CARDS)		+= ath/
 
-obj-$(CONFIG_MAC80211_HWSIM)	+= mac80211_hwsim.o
+obj-$(CONFIG_COMPAT_MAC80211_HWSIM)	+= mac80211_hwsim.o
 
 obj-$(CONFIG_WL_TI)	+= ti/
 
-obj-$(CONFIG_MWIFIEX)	+= mwifiex/
+obj-$(CONFIG_COMPAT_MWIFIEX)	+= mwifiex/
 
